    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csrf-csrf": "^3.1.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "mock-aws-s3": "^4.0.2",
    "moment": "^2.30.1",
    "mongodb": "^5.9.2",
    "mongoose": "^8.8.4",
    "mysql2": "^3.11.5",
    "nock": "^13.5.6",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
